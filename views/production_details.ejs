<!DOCTYPE html>
<html>
<head>
    <% include partials/head.ejs%>
</head>
<body>

<% include partials/header2.ejs %>
<div class="main-blog-single">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 text-center" style="padding-bottom: 30px">
                <h1><%= data[0].title %></h1>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="content-wrap">
                    <article class="post">
                        <div class="content-post padding dots">
                            <%- data[0].content_text %>
                        </div>
                    </article>
                </div>
            </div>

        </div>
        <div class="row justify-content-center">
                <% data[0].gallery.forEach( gal => {%>
                    <div class="col-lg-2 col-md-2 col-sm-12">
                        <img src="/image/production/<%= gal %>" alt="A cantilever arm we manufacture">
                    </div>
                <% })%>

        </div>
    </div>
</div>

<% include partials/footer.ejs %>

<div id="scroll-top">â–²</div>
<% include partials/footerscripts.ejs %>
</body>
</html>
